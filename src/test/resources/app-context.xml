<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-4.1.xsd">

    <context:component-scan base-package="com.jalbersh.sample" />

    <bean id="gs" class="com.jalbersh.sample.service.GeocodingService">
    </bean>

    <bean id="ms" class="com.jalbersh.sample.service.MapService">
    </bean>

    <bean id="os" class="com.jalbersh.sample.service.OrderService">
    </bean>

</beans>